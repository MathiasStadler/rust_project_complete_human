{
    "cSpell.words": [
        "autogen",
        "DHAT",
        "Falsches",
        "flamegraph",
        "ftrace",
        "hwdata",
        "mlock",
        "rustup",
        "schroot",
        "tracepoint",
        "trapapa",
        "usbip",
        "usbutils",
        "valgrind"
    ],
   "todo-tree.highlights.enabled": true,
   "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "NOTE",
    "COMMENT",
    "[ ]",
    "[x]"
],
"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
"todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text-and-comment",
    "foreground": "#000000",
    "background": "#FFFFFF",
    "opacity": 50,
    "iconColour": "#15ff00",
    "gutterIcon": true,
},
"todo-tree.highlights.customHighlight": {
    "TODO": {
        "icon": "check",
        "foreground": "#FFFFFF",
        "background": "#FFFFFF",
        "iconColour": "#FFFFFF",
    },
    "NOTE": {
        "icon": "note",
        "foreground": "#FFFFFF",
        "background": "#FFFFFF",
        "iconColour": "#FFFFFF",
    },
    "COMMENT": {
        "icon": "note",
        "foreground": "#FFFFFF",
        "background": "#888888",
        "iconColour": "#888888",
    },
    "FIXME": {
        "foreground": "#FFFFFF",
        "background": "#fbff00",
        "iconColour": "#fbff00",
    },
    "BUG": {
        "foreground": "#FFFFFF",
        "background": "#FF0000",
        "iconColour": "#FF0000",
    },
    "[ ]": {
        "icon": "check",
        "foreground": "#FFFFFF",
        "background": "#FF0000",
        "iconColour": "#FF0000",
    },
    "[x]": {
        "icon": "check",
        "foreground": "#FFFFFF",
        "background": "#79f108",
        "iconColour": "#79f108",
    }
},
}
